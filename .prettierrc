{
    "useTabs": true,
    "singleQuote": false,
    "trailingComma": "none",
    "printWidth": 100,
    "pluginSearchDirs": [
        "."
    ],
    "plugins": [
        "prettier-plugin-svelte",
        "prettier-plugin-tailwindcss"
    ],
    "overrides": [
        {
            "files": "*.svelte",
            "options": {
                "parser": "svelte",
                "printWidth": 80
            }
        },
        {
            "files": "*.md",
            "options": {
                "parser": "markdown",
                "printWidth": 79
            }
        }
    ]
}